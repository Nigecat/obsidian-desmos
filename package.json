{
    "name": "obsidian-desmos",
    "version": "0.5.9",
    "description": "Embed Desmos graphs into your notes",
    "main": "main.js",
    "author": "Nigecat",
    "scripts": {
        "build": "rollup --config rollup.config.js --environment BUILD:production",
        "build:dev": "rollup --config rollup.config.js -w",
        "lint": "eslint src --ext .ts",
        "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.spec.ts'",
        "test:clean": "npm run build && npm test"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-typescript": "^8.2.1",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^15.12.2",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "chai": "^4.3.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsdoc": "^38.0.6",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "mocha": "^9.2.2",
        "obsidian": "^0.12.5",
        "rollup": "^2.51.2",
        "ts-node": "^10.7.0",
        "tslib": "^2.3.0",
        "typescript": "^4.3.2"
    }
}
