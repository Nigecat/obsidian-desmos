{
    "name": "obsidian-desmos",
    "version": "0.6.4",
    "description": "Embed Desmos graphs into your notes",
    "main": "main.js",
    "author": "Nigecat",
    "scripts": {
        "build": "rollup --config rollup.config.js --environment BUILD:production",
        "build:dev": "rollup --config rollup.config.js -w",
        "lint": "eslint src --ext .ts",
        "fmt": "prettier --write src",
        "fmt:check": "prettier --check src",
        "test": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} npm run test:clean",
        "test:raw": "mocha --parallel -r ts-node/register 'tests/**/*.spec.ts'",
        "test:clean": "npm run build && npm run test:raw",
        "test:coverage": "nyc --reporter html --reporter text npm test"
    },
    "dependencies": {
        "mathjs": "^11.7.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-typescript": "^8.2.1",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^15.12.2",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "chai": "^4.3.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsdoc": "^38.0.6",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "mocha": "^9.2.2",
        "nyc": "^15.1.0",
        "obsidian": "^0.16.3",
        "prettier": "^2.6.2",
        "puppeteer": "^14.0.0",
        "rollup": "^2.51.2",
        "rollup-plugin-copy": "^3.4.0",
        "ts-node": "^10.7.0",
        "tslib": "^2.3.0",
        "typescript": "^4.6.3"
    }
}
